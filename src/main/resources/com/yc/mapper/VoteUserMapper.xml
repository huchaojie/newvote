<mapper namespace="com.yc.mapper.VoteUserMapper">
	<select id="login" parameterType="com.yc.bean.VoteUser" resultType="com.yc.bean.VoteUser">
		select * from voteuser where uname=#{uname} and pwd=#{pwd}
	</select>
	
</mapper>